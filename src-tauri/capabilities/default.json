{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "log:default",
    "fs:read-all",
    {
      "identifier": "fs:scope",
      "allow": [
        "C:/**",
        "D:/**",
        "E:/**",
        "$HOME/**"
      ]
    }
  ]
}
